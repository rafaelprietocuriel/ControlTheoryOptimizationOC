function out=$MODELNAME$NonsmoothFunction($INDEPENDENT$,$DEPENDENTVAR$,$PARVAR$,$ARCVAR$)
% returns the value of the nonsmooth functions
$INFODETAILS$
	
$PARAMETERVALUES$
	
