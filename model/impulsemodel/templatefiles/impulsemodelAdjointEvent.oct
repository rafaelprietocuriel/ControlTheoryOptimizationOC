function out=$MODELNAME$AdjointEvent($JUMPTIME$,$LRDEPENDENTVAR$,$PARVAR$,$ARCVAR$,$JUMPVAR$)
% returns the optimal impulse control values for $MODELNAME$
$INFODETAILS$
	
$PARAMETERVALUES$
	
$LRDEPENDENTVAR2L$
$LRDEPENDENTVAR2R$
	
$IMPULSECONTROL$=$MODELNAME$OptimalImpulseControl($JUMPTIME$,$LRDEPENDENTVAR$,$PARVAR$,$ARCVAR$,$JUMPVAR$);
	
!IF $NUMBEROFJUMPIDS$!
switch $ARCVAR$
!STARTADDARCCASE!
	case $ARCIDENTIFIERCONTROL$
		$ADJOINTEVENTVALUE$
!ENDADDARCCASE!
end
!ENDIF!
!IF ~$NUMBEROFJUMPIDS$!
$ADJOINTEVENTVALUE$
!ENDIF!